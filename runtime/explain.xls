query: SELECT cl.name AS clientName,SUM(i.sum) AS executed,t.sum - SUM(i.sum) AS execRemind,t.contract_num,t.client_id,t.sum,t.payed,t.date,t.term_start,t.term_end,t.type,t.id FROM contracts AS t LEFT JOIN clients AS cl ON t.client_id=cl.id LEFT JOIN invoices AS i ON t.contract_num=i.contract_num WHERE t.date>= ? AND t.date<= ? GROUP BY contract_num
